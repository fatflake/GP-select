@Inproceedings{sparseGP,
abstract     = {<p>We present a framework for sparse Gaussian process (GP) methods which uses
                forward selection with criteria based on information-theoretical principles,
                previously suggested for active learning. In contrast to most previous work on
                sparse GPs, our goal is not only to learn sparse predictors (which can be
                evaluated in O(d) rather than O(n), d ll  n, n the number of training points),
                but also to perform training under strong restrictions on time and memory
                requirements. The scaling of our method is at most O(n d<sup>2</sup>), and in
                large real-world classification experiments we show that it can match prediction
                performance of the popular support vector machine (SVM), yet it requires only a
                fraction of the training time. In contrast to the SVM, our approximation produces
                estimates of predictive probabilities (`error bars'), allows for Bayesian model
                selection and is less complex in implementation.</p>},
author       = {Neil Lawrence and Matthias Seeger and Ralf Herbrich},
booktitle    = {Advances in Neural Information Processing Systems 15},
month        = {January},
pages        = {625-632},
publisher    = {MIT Press},
title        = {Fast Sparse Gaussian Process Methods: The Informative Vector Machine},
url          = {http://research.microsoft.com/apps/pubs/default.aspx?id=76038},
year         = {2003},
}


