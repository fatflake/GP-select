	Pmid = {20068583},
	Publisher = {Nature Publishing Group},
	Shorttitle = {Nat Rev Neurosci},
	Title = {{The free-energy principle: a unified brain theory?}},
	Url = {http://dx.doi.org/10.1038/nrn2787},
	Volume = {11},
	Year = {2010},
	Bdsk-Url-1 = {http://dx.doi.org/10.1038/nrn2787}}

